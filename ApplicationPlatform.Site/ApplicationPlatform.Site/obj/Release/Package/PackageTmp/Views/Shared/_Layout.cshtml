<!DOCTYPE html>
<html lang="zh">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <script src="~/Content/jquery-easyui-1.4.1/jquery-1.8.2.js"></script>
    <script src="~/Content/jquery-easyui-1.4.1/jquery.easyui.min.js"></script>
    <script src="~/Content/jquery-easyui-1.4.1/locale/easyui-lang-en.js"></script>
    <script src="~/Content/tableExporter/tableExporter.js"></script>
    <link href="~/Content/jquery-easyui-1.4.1/themes/default/easyui.css" rel="stylesheet" />
    <link href="~/Content/jquery-easyui-1.4.1/themes/icon.css" rel="stylesheet" />
    <link href="~/Content/jquery-easyui-1.4.1/themes/color.css" rel="stylesheet" />
    <link href="~/Content/flatpickr-gh-pages20160929/dist/flatpickr.min.css" rel="stylesheet" />
    <script src="~/Content/flatpickr-gh-pages20160929/dist/flatpickr.js"></script>
    <title>@ViewBag.Title - Hexagon Customer Requirement Management System</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    
    <script type="text/javascript">


    </script>
</head>
<body class="easyui-layout">

    @RenderBody()
    @RenderSection("scripts", required: false)
    <footer style="height:5% ;text-align:center;background-color:#E2E2E2">
        <div class="content-wrapper">
            <div style="padding-top:2px">
                <p style="font-size:12px;">&copy; @DateTime.Now.Year - Hexagon Customer Requirement Management System</p>
            </div>
        </div>
    </footer>

</body>
</html>
